    <style>
        *,
        ::after,
        ::before {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        .font {
            font-family: "Baloo 2", cursive;
            font-family: "Roboto", sans-serif;
            font-family: "Source Sans Pro", sans-serif;
            font-family: "Work Sans", sans-serif;
        }

        .font-upper{
            font-family: "Teko", sans-serif;
        }

        .wrap {
            padding: 0rem 3rem;
            position: relative;
            width: 10%;
            height: 100%;
            display: grid;
            justify-content: center;
            align-items: center;
        }

        .wrap::after {
            content: "";
            transition: transform 0.5s ease-out;
            background-color: white;
            height: 2px;
            border-radius: 10px;
            width: 90%;
            bottom: 0;
            left: 5%;
            transform: scaleX(0);
            position: absolute;
            transform-origin: right center;
        }

        .wrap:hover.wrap::after {
            transform: scaleX(1);
            transform-origin: center left;
            transition-duration: 0.5s;
        }


        .right-arrow {
            transition: all 0.6s;
            left: 60%;
            position: absolute;
        }

        .btn:hover .right-arrow {
            left: 100%;
        }

        .btn:hover .left-arrow {
            left: 10%;
        }

        .left-arrow {
            transition: all 0.6s;
            position: absolute;
            left: -50%;
        }


        i {
            margin: 1rem;
        }

        .email:hover p {
            text-decoration: underline;
        }

        .address:hover p {
            text-decoration: underline;
        }

        .phone:hover p {
            text-decoration: underline;
        }

        .email:hover .icons {
            transition: 0.4s ease-in-out;
            background-color: #101335;
        }

        .address:hover .icons {
            transition: 0.4s ease-in-out;
            background-color: #101335;
        }

        .phone:hover .icons {
            transition: 0.4s ease-in-out;
            background-color: #101335;
        }

        .footer-main{
            background-image: url(/image/footer-bg.png);
        }

        li {
            position: relative;
        }

        .liner ul li a::after {

            content: "";
            position: absolute;
            width: 100%;
            height: 2px;
            background-color: #ff6e01;
            transform: scaleX(0);
            bottom: 0;
            left: 0 ;
            transform-origin: left center ;
            transition: 0.4s ease-in-out;
            
        }

        .liner ul li:hover a::after {
            
            color: white;
            transform: scaleX(1);
            transition: 0.4s ease-in-out;
        }

        .liner ul li:hover a {
            color: white;
        }

        .social{
            transition: 0.4s ease-in-out;
        }

        .copy-right::before {
            content: "";
            width: 100%;
            height: 1px;
            position: absolute;
            top: 0;
            background:linear-gradient(90deg,#272121,#999b9f,#272121);

        }
        
        .contact-us-inner{
            transition: 0.3s ease-in;
            z-index: -1;
            top: 105%;
            transform: translateY(-15px);
            box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.336);
        }
        
        .contact-active{
            z-index: 1 !important;
            transform: translateY(0px)!important;
            opacity: 1 !important;
        }

        rect{
            transition: 0.6s;
            stroke-dasharray: 100;
            stroke-dashoffset: 15;
        }

        svg:hover rect{
            stroke: #ff6e01;
            stroke-dashoffset: 210;
            /* stroke-dashoffset: 100; */
            
        }


    </style>
